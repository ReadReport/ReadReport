<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             android:layout_width="match_parent"
                                             android:layout_height="wrap_content"
    >


    <com.wy.report.widget.view.SuperEditText
        android:id="@+id/login_account"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="40dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:drawablePadding="15dp"
        android:text="1586368456"
        android:textColor="@color/hei_333333"
        android:textSize="17sp"
        app:delete_height="15"
        app:delete_width="15"
        app:delete_x="0"
        app:delete_y="0"
        app:ic_delete="@drawable/btn_delete_normal"
        app:ic_left_click="@drawable/ic_modify_phone"
        app:ic_left_unclick="@drawable/ic_modify_phone"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:left_height="15"
        app:left_width="15"
        app:left_x="0"
        app:left_y="0"
        app:lineColor_click="@color/hei_333333"
        app:lineColor_unclick="@color/hei_333333"
        app:linePosition="1"
        />

    <com.wy.report.widget.view.SuperEditText
        app:layout_constraintTop_toBottomOf="@+id/login_account"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:drawablePadding="15dp"
        android:text="1586368456"
        android:textColor="@color/hei_333333"
        android:textSize="17sp"
        app:delete_height="15"
        app:delete_width="15"
        app:delete_x="0"
        app:delete_y="0"
        app:ic_delete="@drawable/btn_password_open"
        app:ic_left_click="@drawable/ic_modify_password"
        app:ic_left_unclick="@drawable/ic_modify_password"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:left_height="15"
        app:left_width="15"
        app:left_x="0"
        app:left_y="0"
        app:lineColor_click="@color/hei_333333"
        app:lineColor_unclick="@color/hei_333333"
        app:linePosition="1"
        />

