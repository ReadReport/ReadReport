<?xml version="1.0" encoding="utf-8"?>
<com.wy.report.widget.view.SwipeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/swipe"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/vh_daily_detect_data_list_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/vh_daily_detect_data_list_item_value0"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="124/86"
                    android:textColor="@color/hui_575757"
                    android:textSize="14sp"/>

                <TextView
                    android:id="@+id/vh_daily_detect_data_list_item_state0"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="血压偏高"
                    android:textColor="@color/hui_575757"
                    android:textSize="14sp"/>

                <TextView
                    android:id="@+id/vh_daily_detect_data_list_item_time0"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="2017-11-11"
                    android:textColor="@color/hui_575757"
                    android:textSize="14sp"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/vh_daily_detect_data_list_item_value1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="124/86"
                    android:textColor="@color/hui_575757"
                    android:textSize="14sp"/>

                <TextView
                    android:id="@+id/vh_daily_detect_data_list_item_state1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="血压偏高"
                    android:textColor="@color/hui_575757"
                    android:textSize="14sp"/>

                <TextView
                    android:id="@+id/vh_daily_detect_data_list_item_time1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="2017-11-11"
                    android:textColor="@color/hui_575757"
                    android:textSize="14sp"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/vh_daily_detect_data_list_item_value2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="124/86"
                    android:textColor="@color/hui_575757"
                    android:textSize="14sp"/>

                <TextView
                    android:id="@+id/vh_daily_detect_data_list_item_state2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="血压偏高"
                    android:textColor="@color/hui_575757"
                    android:textSize="14sp"/>

                <TextView
                    android:id="@+id/vh_daily_detect_data_list_item_time2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="2017-11-11"
                    android:textColor="@color/hui_575757"
                    android:textSize="14sp"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/vh_daily_detect_data_list_item_value3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="124/86"
                    android:textColor="@color/hui_575757"
                    android:textSize="14sp"/>

                <TextView
                    android:id="@+id/vh_daily_detect_data_list_item_state3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="血压偏高"
                    android:textColor="@color/hui_575757"
                    android:textSize="14sp"/>

                <TextView
                    android:id="@+id/vh_daily_detect_data_list_item_time3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="2017-11-11"
                    android:textColor="@color/hui_575757"
                    android:textSize="14sp"/>
            </LinearLayout>

        </LinearLayout>


        <TextView
            android:id="@+id/vh_daily_detect_data_list_delete"
            android:layout_width="90dp"
            android:layout_height="match_parent"
            android:background="@color/hong_f54f52"
            android:gravity="center"
            android:text="@string/message_delete"
            android:textColor="@color/bai_ffffff"
            android:textSize="14sp"/>
    </LinearLayout>
</com.wy.report.widget.view.SwipeLayout>
